{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://github.com/monkeysppp/VBCompetitions-API/1.0.0/clubUpdate",
  "description": "Details on a club",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "name": {
      "description": "The name for the club",
      "type": "string",
      "minLength": 1,
      "maxLength": 1000
    },
    "notes": {
      "description": "Free form string to add notes about a club.  This can be used for arbitrary content that various implementations can use",
      "type": "string",
      "minLength": 1
    }
  }
}
